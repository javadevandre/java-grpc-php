{
  "name": "grpc/grpc",
  "type": "library",
  "description": "gRPC library for PHP",
  "keywords": ["rpc"],
  "homepage": "https://grpc.io",
  "license": "Apache-2.0",
  "version": "1.42.0",
  "require": {
    "php": ">=7.0.0",
    "google/protobuf": "3.15.5",
    "ext-grpc": "^1.42"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "google/auth": "^v1.3.0"
  },
  "suggest": {
    "ext-protobuf": "For better performance, install the protobuf C extension.",
    "google/protobuf": "To get started using grpc quickly, install the native protobuf library."
  },
  "autoload": {
    "psr-4": {
      "GPBMetadata\\": "src/main/GPBMetadata/",
      "Com\\Migration\\Poc\\": "src/main/Com/Migration/Poc",
      "Grpc\\": "src/lib/"
    }
  }
}
